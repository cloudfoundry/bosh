check process job_with_post_start_script
  with pidfile /var/vcap/sys/run/job_with_post_start_script.pid
  start program "/var/vcap/jobs/job_with_post_start_script/bin/job_with_post_start_script_ctl start"
  stop program "/var/vcap/jobs/job_with_post_start_script/bin/job_with_post_start_script_ctl stop"
  group vcap
