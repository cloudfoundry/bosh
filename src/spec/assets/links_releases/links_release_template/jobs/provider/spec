    description: "description for b"
  c:
    description: "description for c"
    default: "default_c"
  nested.one:
    description: "description for nested.one"
    default: "default_nested.one"
  nested.two:
    description: "description for nested.two"
    default: "default_nested.two"
  nested.three:
    description: "description for nested.three"
